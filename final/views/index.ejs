<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
<table>
    <tr>
        
    </tr>
    <% users.forEach(user => { %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.age %></td>
            <td>
                <a href="/edit/<%= user.id %">Editar</a>
                <a href="/delete/<%= user.id %">Eliminar</a>
            </td>
        </tr>
</table>

</body>
</html>